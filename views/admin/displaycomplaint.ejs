<%- include('./layoutes/header.ejs') %>
    <%- include('./layoutes/sidebar.ejs') %>

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper bg-warning ">
            <!-- Content Header (Page header) -->
            <div class="content-header">

            </div>
            <!-- /.content-header -->

            <!-- Main content -->
            <!-- /.card -->

            <div class="card">
                <div class="card-header">
                    <h3 class="card-title">Display Complaint</h3>
                    <button type="button" class="btn btn-info" style="float: right;" data-toggle="modal"
                        data-target="#addstudent">
                        Add Student
                    </button>
                </div>
                <!-- /.card-header -->
                <div class="card-body">
                    <table id="example1" class="table table-bordered table-striped">
                        <thead>
                            <tr>
                                <th>Id</th>
                                <th>Name</th>
                                <th>Complaint Type</th>
                                <th>Subject</th>
                                <th>Complaint Detail</th>
                                <th>Action</th>

                            </tr>
                        </thead>
                        <tbody>
                            <% i=1 %>
                                <% c.forEach((item)=>{%>
                                    <tr>
                                        <td>
                                            <%= i++ %>
                                        </td>
                                        <td>
                                            <%= item.name %>
                                        </td>

                                        <td>
                                            <%= item.ctype %>
                                        </td>
                                        <td>
                                            <%= item.subject %>
                                        </td>
                                        <td>
                                            <%= item.cdetail %>
                                        </td>

                                        <td>
                                            <a href="/admin/studentview/<%= item._id%>" class="btn btn-info">View</a>
                                            <a href="/admin/studentdelete/<%= item._id%>"class="btn btn-danger">Delete</a>
                                        </td>
                                    </tr>
                                    <%})%>

                        </tbody>
                        <tfoot>
                            <tr>
                                <th>Id</th>
                                <th>Name</th>
                                <th>Complaint Type</th>
                                <th>Subject</th>
                                <th>Complaint Detail</th>
                                <th>Action</th>
                            </tr>
                        </tfoot>
                    </table>
                </div>
                <!-- /.card-body -->
            </div>
            <!-- /.card -->

            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->


        <%- include('./layoutes/footer.ejs') %>